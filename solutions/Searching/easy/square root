class Solution {
    public int mySqrt(int x) {
if(x==0||x==1)
return x;
     int start=0, end=x/2,sol=0;
      
            while(start<=end){

                 int mid=start+(end-start)/2;
                if(mid*mid<=x){
                    start=mid+1;
                   sol=mid;
              
                }
                else{
                    end=mid-1;
                }
               
            }
 return sol;
    }
}